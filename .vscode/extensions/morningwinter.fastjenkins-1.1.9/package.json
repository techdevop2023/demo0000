{
	"name": "fastjenkins",
	"displayName": "Fast Jenkins",
	"version": "1.1.9",
	"description": "conveniently access jenkins and build your jobs.",
	"main": "./dist/extension.js",
	"author": "MorningWinter",
	"license": "MIT",
	"publisher": "morningwinter",
	"icon": "static/jenkins.png",
	"categories": [
		"Other"
	],
	"keywords": [
		"muyuan",
		"jenkins",
		"fast",
		"morningwinter",
		"build"
	],
	"repository": {
		"type": "git",
		"url": "https://gitee.com/daffodil/fastjenkins/"
	},
	"activationEvents": [
		"onView:fastJenkins.sidebarView"
	],
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "fast-jenkins",
					"title": "Fast Jenkins: 快速部署",
					"icon": "static/jenkins.svg"
				}
			]
		},
		"views": {
			"fast-jenkins": [
				{
					"id": "fastJenkins.sidebarView",
					"type": "webview",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "sidebarView.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "static/light/refresh.svg",
					"dark": "static/dark/refresh.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "sidebarView.refreshEntry",
					"when": "view == fastJenkins.sidebarView",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"title": "FastJenkins",
			"properties": {
				"fastjenkins.apiToken": {
					"type": "string",
					"default": null,
					"description": "Jenkins 用户 API Token: 通过登录jenkins服务 -> 用户设置获取"
				},
				"fastjenkins.userId": {
					"type": "string",
					"default": null,
					"description": "Jenkins 用户 ID: 登录jenkins服务的账户"
				},
				"fastjenkins.domain": {
					"type": "string",
					"default": null,
					"description": "Jenkins 域名: 可以使用 https://IP:port 形式"
				}
			}
		}
	},
	"engines": {
		"vscode": "^1.66.0"
	},
	"scripts": {
		"vscode:prepublish": "node build/package.js",
		"build": "node build/package.js",
		"package": "vsce package --no-yarn",
		"publish": "vsce publish --no-yarn",
		"watch": "node build/preLaunchTask.js",
		"lint:prettier": "prettier --write ."
	},
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"nprogress": "^0.2.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@types/node": "^17.0.23",
		"@types/nprogress": "^0.2.0",
		"@types/react": "^18.0.5",
		"@types/react-dom": "^18.0.1",
		"@types/vscode": "^1.66.0",
		"antd": "^4.19.5",
		"axios": "^0.26.1",
		"esbuild": "^0.14.30",
		"esbuild-plugin-less": "^1.1.6",
		"less": "^4.1.2",
		"prettier": "^2.6.2",
		"typescript": "^4.6.3"
	},
	"__metadata": {
		"id": "01931f80-ff70-4e7a-a7bb-21688be11c04",
		"publisherId": "b1403502-3d6a-4fc3-b78c-4276d9b57950",
		"publisherDisplayName": "morningwinter",
		"targetPlatform": "undefined",
		"updated": false,
		"isPreReleaseVersion": false,
		"installedTimestamp": 1699323648076,
		"preRelease": false
	}
}